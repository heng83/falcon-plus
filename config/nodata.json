{
    "debug": true,
    "http": {
        "enabled": false,
        "listen": "%%NODATA_HTTP%%"
    },
    "plus_api":{
        "connectTimeout": 500,
        "requestTimeout": 2000,
        "addr": "%%PLUS_API_HTTP%%",
        "token": "%%PLUS_API_DEFAULT_TOKEN%%"
    },
    "config": {
        "enabled": true,
        "dsn": "%%MYSQL%%/falcon_portal?loc=Local&parseTime=true&wait_timeout=604800",
        "maxIdle": 4
    },
    "collector":{
        "enabled": true,
        "batch": 200,
        "concurrent": 10
    },
    "sender":{
        "enabled": true,
        "connectTimeout": 500,
        "requestTimeout": 2000,
        "transferAddr": "%%TRANSFER_HTTP%%",
        "batch": 500
    }
}
